                        customerUniqueIdentifier: localStorage.getItem("analyticsCustomerIdentifier") ?? "NA",
                    },
                    page: getPageDetails()
                };
                window.digitalData.push(pageData);
                _satellite.track("OTP Requested");
            }
        }
        if(otpState == 'success') {
            if (window.digitalData) {
                const pageData = {
