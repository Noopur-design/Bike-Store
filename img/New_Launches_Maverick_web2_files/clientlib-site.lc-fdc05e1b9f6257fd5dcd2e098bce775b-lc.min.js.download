                let submitted_ans = child.answer;
                let jsonData = {
                  id: ele.id,
                  question: ele.question,
                  correctAnswer: ele[correct_ans],
                  answer: ele[submitted_ans],
                  iscorrect: child.iscorrect,
                };
                resultJson.push(jsonData);
              }
            });
        });
      console.log(resultJson);
