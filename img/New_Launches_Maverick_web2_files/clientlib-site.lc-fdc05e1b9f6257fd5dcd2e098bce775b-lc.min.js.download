  function isNumeric(e) {
    let charCode = e.which ? e.which : e.keyCode;
    if (
      e.type === "blur" ||
      e.type === "touchend" ||
      (charCode > 95 && charCode < 106) ||
      (charCode > 47 && charCode < 58) ||
      charCode == 8 ||
      charCode == 9
    ) {
      return true;
    }
    e.preventDefault();
    return false;
  }
  function isAlphaAndSpace(e) {
    let charCode = e.which ? e.which : e.keyCode;
    if (
      e.type === "blur" ||
      e.type === "touchend" ||
      (charCode > 96 && charCode < 123) ||
      (charCode > 64 && charCode < 91) ||
      charCode == 32 ||
      charCode == 8 ||
      charCode == 9
    ) {
      return true;
    }
    e.preventDefault();
    return false;
  }
