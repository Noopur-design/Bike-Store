        }
    }

    function sendFormStartEvent(formValues) {
        if (window.digitalData) {
            const pageData = {
                event: "Form Start",
                formDetails: formValues,
                user: {
                    hashedEmail: localStorage.getItem("analyticsHashedEmail") ?? "NA",
