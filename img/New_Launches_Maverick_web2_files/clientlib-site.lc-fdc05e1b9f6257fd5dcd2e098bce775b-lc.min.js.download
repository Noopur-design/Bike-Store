$(document).ready(function () {
    let html = $("html");
    let wrapper = $(".vertical-wrapper");
    let stickyMob = $('#horizontal-sticky-mob');
    let arrow = stickyMob.find('.arrow');
    let submenu = $('.submenu');
    let arrowImg = arrow.find('img');

