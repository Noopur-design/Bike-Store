      ).on("change", function () {
        audio.currentTime = this.value;
      });

      // fires when page loads, it sets the min and max range of the video
      function setSeekBar(parentElement) {
        const seekbar = parentElement.find(".audio-seekbar");
        if (seekbar.length) {
          seekbar[0].min = 0;
          seekbar[0].max = audio.duration;
