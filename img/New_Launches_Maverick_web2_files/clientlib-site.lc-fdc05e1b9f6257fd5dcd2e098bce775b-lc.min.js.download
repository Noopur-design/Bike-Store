      if (textLength > 2000) {
        $(this).val($(this).val().substring(0, 2000));
      }
    });

    function sortAscending(a, b) {
      return $(b).text().toUpperCase() < $(a).text().toUpperCase() ? 1 : -1;
    }

    $("#testride").change(function () {
      if (this.checked) {
        $("#test--date").show();
        $("#test--date input").attr("required", true);
      } else {
