    function exchangePincodeValidation(value) {
        const disallowedPincodes = [
            "012345",
            "111111",
            "222222",
            "333333",
            "444444",
            "555555",
            "666666",
            "777777",
            "888888",
            "999999",
            "000000",
            "123456",
            "101010",
            "202020",
            "303030",
            "404040",
            "505050",
        ];
        const regexPincode = /^[1-9][0-9]{5}$/;
        if (disallowedPincodes.indexOf(value) !== -1) {
            return false;
        } else {
            return regexPincode.test(value);
        }
    }

